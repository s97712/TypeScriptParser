<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <PackageId>TypeScriptParser</PackageId>
    <Description>TypeScript语法解析器 - 基于Tree-sitter的.NET绑定</Description>
    <PackageTags>tree-sitter;typescript;parser;syntax;ast</PackageTags>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
  </PropertyGroup>

  <!-- Native库依赖 -->
  <ItemGroup>
    <PackageReference Include="TypeScriptParser.Native" Version="$(Version)" />
  </ItemGroup>

</Project>
